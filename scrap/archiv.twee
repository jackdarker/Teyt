.gf-image-wrapper {
	max-width: 50%;
  	border: 1px hidden;
  	border-radius: 3px;
}

.gf-image-wrapper img {
	width: 100%;
}


.default-image-wrapper {
	max-width: 50%;
  	border: 1px hidden;
}

.default-image-wrapper img {
	width: 100%;
}

.default-image-wrapper-clothes img {
	width: 25%;
  	border: 1px hidden;
}

.default-image-wrapper-clothes-shop img {
	width: 17%;
  	border: 1px hidden;
}

img.full-width-image  {
	width: 100%;
}

.char-male {
	background-color: #deeaff;
  	color: #7096ff;
  	padding: 5px;
  	border: 1px solid #8490c9;
}

.image-center {
  display: block;
  max-width: 50%;
  margin: 0 auto;
}

.image-full-center {
  display: block;
  max-width: 80%;
  margin: 0 auto;
}

.image-center img {
  width: 100%;
}

.image-full-center img {
  width: 100%;
}

.image-wrapper-w70p {
  	display: block;
	width: 100%;
  	text-align: center;
}


.image-wrapper-w50p {
  	display: block;
	width: 100%;
  	text-align: center;
}

.image-wrapper-small img {
  	display: block;
	max-width: 30%;
  	border: 1px hidden;
}

.image-wrapper-w70p img {
	width: 70%;
}

.image-wrapper-w50p img {
	width: 50%;
}

/* styles that overlap native  */
#passages {
	max-width: 54em;
	margin: 0 auto;
  	font-size: 120%;
}

.progressBar {
  width: 200px;
  height: 6px;
  background-color: #f0a3a3;
}

#setting-body-gender {
  display: none;
}

/* Main character styles */
.mc-text {
    display: block;
	color: #fe4bd0;
  	background-color: #ffd0f6;
  	width: 100%;
  	padding: 10px;
  	border-radius: 5px;
  	margin-top: 5px;
}
.mc-text.thought {
  font-style: italic;
}

.mc-arousal-up {
  	display: block;
  	padding: 5px;
  	margin-bottom: 5px;
  	text-align: center;
  	background-color: #333;
  	border-radius: 3px;
  	border: 1px solid #444;
	color: #ff1fc5;
}

.mc-arousal-down {
  	display: block;
  	padding: 5px;
  	margin-bottom: 5px;
  	text-align: center;
  	background-color: #333;
  	border-radius: 3px;
  	border: 1px solid #444;
	color: #b2b1ff;
}

.mc-attr-up {
  	display: block;
  	padding: 5px;
  	margin-bottom: 5px;
  	text-align: center;
  	background-color: #333;
  	border-radius: 3px;
  	border: 1px solid #444;
	color: #59fc69;
}

.mc-attr-down {
  	display: block;
  	text-align: center;
  	padding: 5px;
  	margin-bottom: 5px;
  	background-color: #333;
  	border-radius: 3px;
  	border: 1px solid #444;
  	color: #fe3737;
}

/* Unknown character styles */
.uc-text {
    display: block;
	color: #3f3f3f;
  	background-color: #ebebeb;
  	width: 100%;
  	padding: 10px;
  	border-radius: 5px;
  	text-align: right;
  	margin-top: 5px;
}
.uc-text.thought {
  	font-style: italic;
}

/* Brother character styles */
.bc-text {
    display: block;
	color: #eaffff;
  	background-color: #8795ff;
  	width: 100%;
  	padding: 10px;
  	border-radius: 5px;
  	text-align: right;
  	margin-top: 5px;
}
.bc-text.thought {
  	font-style: italic;
}

.narrator {
    display: block;
  	padding: 10px;
  	width: 100%;
	color: #ffdefe;
  	font-style: italic;
  	margin-top: 5px;
}

.wardrobe-clothes td,
.wardrobe-clothes th {
	text-align: center;
}

.wardrobe-clothes img {
	max-width: 50%;
}

.male-text {
	color: dodgerblue;
}

.female-text {
	color: hotpink;
}







