:: Clothes Item Widget [widget nobr system] {"position":"800,1300","size":"100,100"}
<<widget "ClothesItem">>
	<!-- $args[0] - name of the clothes item, i.e. "body", "legs" -->
	<table width="100%" border="5" class="wardrobe-clothes">
		<caption><<= $args[0].toLocaleUpperFirst() >> clothes</caption>
			<tbody>
				<<for _itemTitle, _item range $wardrobe.clothes[$args[0]]>>
					<tr>
						<td width="20%">
							<<capture _itemTitle>>
								<<if $wardrobe.clothes[$args[0]][_itemTitle].name === $mainChar.look.clothes[$args[0]].name>>
									<<set _clothesName = $wardrobe.clothes[$args[0]][_itemTitle].name + " (take off)" >>
									<<link _clothesName "Wardrobe">>
										<<set $mainChar.look.clothes[$args[0]] = $emptyItem >>
									<</link>>
								<<else>>
									<<set _clothesName = $wardrobe.clothes[$args[0]][_itemTitle].name + " (wear)" >>
									<<link _clothesName "Wardrobe">>
										<<set $mainChar.look.clothes[$args[0]] = $wardrobe.clothes[$args[0]][_itemTitle] >>
									<</link>>
								<</if>>
							<</capture>>
						</td>
						<td align="center">
							<div class="default-image-wrapper-clothes">
							[img[$wardrobe.clothes[$args[0]][_itemTitle].image]]
							</div>
						</td>
						<td width="20%">
							<<= $wardrobe.clothes[$args[0]][_itemTitle].description >>
						</td>
					</tr>
				<</for>>
			</tbody>
	</table>
	<br />
	<br />
	[[Back->Wardrobe]]
<</widget>>


:: Clothes Item In The Market Widget [widget nobr] {"position":"2600,1000","size":"100,100"}
<<widget "ClothesItemInTheMarket">>
	<!-- $args[0] - market clothes item object -->
	<!-- $args[1] - market clothes item title -->
	* <<link $args[0].name "Lexena Market Clothes Item View">>
		<<set $tempClothesItem = $args[0] >>
	<</link>> for $
	<<if settings.lessPrices == true>>
		<<if $wereClothesShopPricesReduced != true>>
			<<set $args[0].price /= 2>>
		<<endif>>
	<<else>>
		<<if $wereClothesShopPricesReduced == true>>
			<<set $args[0].price *= 2>>
		<<endif>>
	<<endif>>
	<<=$args[0].price>> - 
	<<if $wardrobe.available.includes($args[0].name)>>
		You already have this!
	<<else>>
		<<if $mainChar.money < $args[0].price>>
			You don't have enough money!
		<<else>>
			<<link "Buy">>
				<<set $minute += 1>>
				<<set $mainChar.money -= $args[0].price>>
				<<run $wardrobe.addItem($args[0].type, $args[0]); >>
				<<goto "Clothes Shop">>
			<</link>>
		<<endif>>
	<<endif>>
	<br />
<</widget>>




